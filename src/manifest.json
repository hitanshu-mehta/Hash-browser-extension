{
    "name": "Hash Password Manager",
    "version": "0.1",
    "description": "Manage your password securely",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webNavigation",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "index.html"
    }
}